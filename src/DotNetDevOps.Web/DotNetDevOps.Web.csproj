    <EmbeddedResource Include="Templates\Applications\Carshopper.json" />
    <EmbeddedResource Include="Templates\Applications\EarthML.Pipelines.json" />
    <EmbeddedResource Include="Templates\KeyVault\retrieveAndParseCertificate.json" />
