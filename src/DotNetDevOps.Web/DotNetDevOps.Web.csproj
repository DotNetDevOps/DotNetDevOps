    <IsServiceFabricServiceProject>True</IsServiceFabricServiceProject>
    <TypeScriptToolsVersion>3.1</TypeScriptToolsVersion>
    <RootNamespace>DotNetDevOps</RootNamespace>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Templates\ACI\ContainerInstanceCommand.json" />
    <Content Remove="Templates\Applications\Carshopper.json" />
    <Content Remove="Templates\Applications\DotNetDevOps.json" />
    <Content Remove="Templates\Applications\EarthML.Pipelines.json" />
    <Content Remove="Templates\Applications\FabricGateway.json" />
