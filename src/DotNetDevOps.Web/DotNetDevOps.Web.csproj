    <Content Remove="Templates\ServiceFabric\GenericApplication.json" />
    <Content Remove="Templates\ServiceFabric\msi.json" />
    <Content Remove="Templates\ServiceFabric\root.json" />
    <Content Remove="Templates\ServiceFabric\secret.json" />
    <Content Remove="Templates\ServiceFabric\vault.json" />
    <Content Remove="Templates\ServiceFabric\vmss.json" />
    <Content Remove="Templates\SInnovations.Gateway\Applications.json" />
