{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "type": "string"
    },
    "mergeAppSettings": {
      "type": "object",
      "defaultValue": {
      }
    },
    "removeAppSettings": {
      "type": "object",
      "defaultValue": {
      }
    },
    "deploymentId": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "GetAppSetting",
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "resources": [],
          "outputs": {
            "appsettings": {
              "type": "object",
              "value": "[list(resourceId('Microsoft.Web/sites/config',parameters('name'),'appsettings'),'2018-11-01').properties]"
            }
          }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "dependsOn": [ "GetAppSetting" ],
      "name": "SetAppSetting",
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "resources": [
            {
              "name": "[concat(parameters('name'),'/appsettings')]",
              "type": "Microsoft.Web/sites/config",
              "apiVersion": "2018-11-01",
              "properties": "[intersection(union(reference('GetAppSetting').outputs.appsettings.value,parameters('mergeAppSettings'),parameters('removeAppSettings')),union(reference('GetAppSetting').outputs.appsettings.value,parameters('mergeAppSettings')))]"
            },
            {
              "condition": "[greater(length(parameters('deploymentId')), 0)]",
              "name": "[concat(parameters('name'),'/',parameters('deploymentId'))]",
              "type": "Microsoft.Web/sites/deployments",
              "apiVersion": "2019-08-01",
              "properties": {
                "status": 4,
                "message": "hello",
                //"author": "string",
                "deployer": "DotNetDevOps Arm Templates"
                //"author_email": "string",
                //"start_time": "string",
                //"end_time": "string",
                //"active": "boolean",
                //"details": "string"
              }
            }
          ],
          "outputs": {
            "appsettings": {
              "type": "object",
              "value": "[list(resourceId('Microsoft.Web/sites/config',parameters('name'),'appsettings'),'2018-11-01').properties]"
            }
          }
        }
      }
    }
    
  ]
}